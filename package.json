{
  "name": "tuterra",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node scripts/sync-schema.js && prisma generate --schema=prisma/schema.sqlite.prisma && next dev",
    "dev:fresh": "rm -rf dev.db && node scripts/sync-schema.js && prisma db push --schema=prisma/schema.sqlite.prisma && npm run dev",
    "build": "prisma generate --schema=prisma/schema.prisma && next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate --schema=prisma/schema.prisma",
    "db:push:local": "node scripts/sync-schema.js && prisma db push --schema=prisma/schema.sqlite.prisma",
    "db:migrate:prod": "prisma migrate deploy --schema=prisma/schema.prisma"
  },
  "dependencies": {
    "@auth/core": "^0.34.3",
    "@prisma/client": "^5.7.0",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "bcrypt": "^5.1.1",
    "date-fns": "^2.30.0",
    "date-holidays": "^3.26.5",
    "dotenv": "^17.2.3",
    "framer-motion": "^12.23.26",
    "heic2any": "^0.0.4",
    "jose": "^6.1.3",
    "lucide-react": "^0.555.0",
    "next": "^16.0.7",
    "next-sitemap": "^4.2.3",
    "next-themes": "^0.4.6",
    "nodemailer": "^7.0.11",
    "prisma": "^5.7.0",
    "react": "^18.3.0",
    "react-day-picker": "^9.11.2",
    "react-dom": "^18.3.0",
    "react-easy-crop": "^5.5.6",
    "recharts": "^3.5.0",
    "sonner": "^2.0.7",
    "zod": "^3.22.4",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^20.19.25",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "^15.0.0",
    "sass": "^1.69.5",
    "typescript": "^5"
  }
}
